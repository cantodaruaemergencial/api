{
  "routes": [
    {
      "method": "GET",
      "path": "/external-services",
      "handler": "external-service.find",
      "config": {
        "policies": ["global::isPublic"]
      }
    },
    {
      "method": "GET",
      "path": "/external-services/count",
      "handler": "external-service.count",
      "config": {
        "policies": ["global::isPublic"]
      }
    },
    {
      "method": "GET",
      "path": "/external-services/:id",
      "handler": "external-service.findOne",
      "config": {
        "policies": ["global::isPublic"]
      }
    },
    {
      "method": "POST",
      "path": "/external-services",
      "handler": "external-service.create",
      "config": {
        "policies": []
      }
    },
    {
      "method": "PUT",
      "path": "/external-services/:id",
      "handler": "external-service.update",
      "config": {
        "policies": []
      }
    },
    {
      "method": "DELETE",
      "path": "/external-services/:id",
      "handler": "external-service.delete",
      "config": {
        "policies": []
      }
    }
  ]
}
